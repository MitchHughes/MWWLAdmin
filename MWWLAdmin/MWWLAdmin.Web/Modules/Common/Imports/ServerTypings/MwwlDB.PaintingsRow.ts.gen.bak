
namespace MWWLAdmin.MwwlDB {
    export interface PaintingsRow {
        Id?: string;
        Title?: string;
        OriginalFileName?: string;
        Dimension?: string;
        Workshop?: boolean;
        OriginalAvailable?: boolean;
        OriginalPrice?: number;
        Framed?: boolean;
    }

    export namespace PaintingsRow {
        export const idProperty = 'Id';
        export const nameProperty = 'Title';
        export const localTextPrefix = 'MwwlDB.Paintings';

        export namespace Fields {
            export declare const Id;
            export declare const Title;
            export declare const OriginalFileName;
            export declare const Dimension;
            export declare const Workshop;
            export declare const OriginalAvailable;
            export declare const OriginalPrice;
            export declare const Framed;
        }

        ['Id', 'Title', 'OriginalFileName', 'Dimension', 'Workshop', 'OriginalAvailable', 'OriginalPrice', 'Framed'].forEach(x => (<any>Fields)[x] = x);
    }
}

